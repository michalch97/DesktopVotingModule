<Page x:Class="DesktopVotingModule.VoteResultPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:DesktopVotingModule"
      xmlns:vm="clr-namespace:DesktopVotingModuleViewModel;assembly=DesktopVotingModuleViewModel"
      mc:Ignorable="d" 
      d:DesignHeight="416.8" d:DesignWidth="544"
      Title="Vote">
    <Page.Resources>
        <vm:VoteResultViewModel x:Key="voteView"/>
    </Page.Resources>
    <Grid DataContext="{StaticResource voteView}" >
        <Button Content="Wróć" Command="{Binding GoToVoteSelectCommand}" Background="DarkGray" Foreground="#231F20" FontFamily="Sitka Banner" FontSize="20" FontWeight="Bold" Margin="314,364,10,9.8"/>
        <TextBlock HorizontalAlignment="Left" Margin="140,186,0,0" TextWrapping="Wrap" Text="WYNIKI GŁOSOWANIA" VerticalAlignment="Top" Height="67" Width="317" Foreground="White" FontSize="30"/>
    </Grid>
</Page>
